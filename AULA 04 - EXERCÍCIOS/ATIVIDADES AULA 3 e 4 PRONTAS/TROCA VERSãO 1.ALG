algoritmo "Troca Versão 1"

var

 V1A: inteiro
 V1B: inteiro
 V2A: inteiro
 V2B: inteiro
inicio

      Escreva("Escreva o valor A")
      leia(V1A)
      Escreva("Escreva o valor B")
      leia(V2B)
      V1B <- V2B
      V2A <- V1A
      
  ESCREVA("O valor de A é ", V1B, " e B", V2A)
 
fimalgoritmo