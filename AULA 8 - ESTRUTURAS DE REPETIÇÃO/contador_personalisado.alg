algoritmo "CONTADOR personalisado"

var
   contador,conta,limite: inteiro
   
inicio
    escreval("De quanto em quanto será a contagem: ")
    leia(conta)
    escreval("ATÉ QUANTO: ")
    leia(limite)
    
    contador <- 0


     enquanto (contador <= limite) faca

        escreva (contador, " ")
        contador <- contador + conta

    fimenquanto



fimalgoritmo