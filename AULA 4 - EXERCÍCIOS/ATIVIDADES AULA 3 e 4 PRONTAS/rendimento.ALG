algoritmo "rendimento poupança "

var

 entrada: real
 renda: real
 resultado: real
 total: real
inicio

      Escreva("digite o valor de entrada")
      leia(entrada)

 renda <- 0.7/100
 resultado <- renda*entrada
 total <- resultado+entrada

 ESCREVA("O rendimento foi de ", total )
 
fimalgoritmo