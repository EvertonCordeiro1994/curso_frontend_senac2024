algoritmo "TAREFA02 - INTEIRO MAIOR OU MENOR"


var

numero: inteiro

inicio
    escreval("Digite um número inteiro: ")
    leia(numero)

    se (numero > 10) entao
        escreval ("o número ",numero," que você digito é maior que o número 10.")
    senao
        escreval ("o número ",numero," que você digito é menor que o número 10.")
    fimse

fimalgoritmo